function getTipsRating(amount) {
  // write your code here
  if (amount = 0) {
    return 'terrible';
  }
  else if (0 < amount <= 10) {
    return 'poor';
  }
  else if (10 < amount <= 20) {
    return 'good';
  }
  else if (20 < amount <= 50) {
    return 'great';
  }
  else if (amount > 50) {
    return 'excellent';
  }
}
